<div class="btn_box">
    <a href="/projects/new">Add new Project</a>
</div>
<section>
  <% @projects.each do |project| %>
      <article>
        <h2><%= project.title %> </h2>
        <p><%= project.description %> </p>
        <div class="btn_more">
          <a href="/projects/show?id=<%=project.id%>">More</a>
        </div>
      </article>
  <%end%>

</section>
